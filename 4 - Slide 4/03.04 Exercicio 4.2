#include<stdio.h>

int ehprimo(int num)
{
   int count = 0;
   for(int i = 1; i<=num;i++)
   {
      if(num % i == 0)
      {
         count++;
      }
   }
   if(count == 2)
   {
      return 1;
   }
   else
   {
      return 0;
   }
}
int main (void)
{
   int max;
   printf("Digite um valor para verificar o intervalo entre ele e 1 para descobrir os primos: ");
   scanf("%d",&max);
   for(int i = 1; i<=max; i++)
   {
   if(ehprimo(i) == 1)
   {
      printf("%d\n",i);
   }
   }
   return 0;
}
